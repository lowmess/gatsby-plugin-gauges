{
  "name": "gatsby-plugin-gauges",
  "description": "Gatsby plugin to add Gauges analytics onto a site",
  "version": "0.0.3",
  "main": "index.js",
  "author": "Alec Lomas <alec@lowmess.com>",
  "repository": "https://github.com/lowmess/gatsby-plugin-gauges",
  "bugs": {
    "url": "https://github.com/lowmess/gatsby-plugin-gauges/issues"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1"
  },
  "peerDependencies": {
    "gatsby": "^1.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
